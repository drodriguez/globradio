#ifdef __OBJC__
    #import <Foundation/Foundation.h>
    #import <UIKit/UIKit.h>
#endif

#import "RNFileLogger.h"

#if defined(DEBUG) || defined(BETA)
#  define RNLog(s, ...) NSLog(s,##__VA_ARGS__), [[RNFileLogger sharedLogger] logFile:__FILE__ lineNumber:__LINE__ format:(s),##__VA_ARGS__]
#else
#  define RNLog(s, ...)
#endif